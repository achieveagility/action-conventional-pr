{
  "name": "@aaos/action-conventional-pr",
  "version": "0.1.0",
  "description": "Opinionated PR title validation action built on action-semantic-pull-request",
  "license": "MIT",
  "type": "module",
  "packageManager": "pnpm@10.0.0",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/achieveagility/action-conventional-pr.git"
  },
  "homepage": "https://github.com/achieveagility/action-conventional-pr#readme",
  "scripts": {
    "format:check": "oxfmt --check",
    "lint": "oxlint",
    "build": "tsdown",
    "types:check": "tsc -p tsconfig.json"
  },
  "files": [
    "action.yml",
    "dist/**/*",
    "README.md"
  ],
  "devDependencies": {
    "@types/node": "25.3.0",
    "oxfmt": "latest",
    "oxlint": "latest",
    "tsdown": "latest",
    "typescript": "^5.9.3"
  }
}
